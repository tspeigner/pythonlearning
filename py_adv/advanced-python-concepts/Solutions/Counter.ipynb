    "collapsed": false
   },
   "outputs": [],
   "source": [
    "from collections import Counter\n",
    "\n",
    "with open('Declaration_of_Independence.txt') as f:\n",
    "    doi = f.read()\n",
    "\n",
    "word_list = [word for word in doi.upper().split() if len(word) > 5]\n",
    "    \n",
    "c = Counter(word_list)\n",
    "c.most_common(10)"
   ]
  }
